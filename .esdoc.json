{
  "source": "./",
  "destination": "./doc",
  "plugins": [{
    "name": "esdoc-standard-plugin",
    "option": {
      "accessor": {"access": ["public"], "autoPrivate": true},
      "lint": {"enable": false},
      "coverage": {"enable": false},
      "undocumentIdentifier": {"enable": false},
      "test": {"source": false}
    }
  }]
}
